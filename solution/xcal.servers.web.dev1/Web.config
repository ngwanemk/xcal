  <connectionStrings>
    <add name="reexjungle.xcal.application.server.web.dev1.Properties.Settings.elmah_mysql_db" connectionString="Server=localhost;Uid =dev;Pwd=dev;Database=xcal_elmah_dev1_db;allow user variables=true" />
